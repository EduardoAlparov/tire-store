<section class="section home">
    <div class="home__background">
        <picture>
            <source media="(max-aspect-ratio: 4/3)" type="image/webp" srcset="./img/home/slide-m.webp" />
            <source media="(min-aspect-ratio: 4/3)" type="image/webp" srcset="./img/home/slide.webp" />
